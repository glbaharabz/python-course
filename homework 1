nums = list(range(30))
teksay覺lar= [i for i in nums if i % 2 ==1]
ciftsay覺lar= [i for i in nums if i % 2== 0 ]
tamsayilar = [i for i in nums]
tamsayilarinikikati = [i*2 for i in nums]

print(teksay覺lar)
print(ciftsay覺lar)
print(tamsayilar)
print(tamsayilarinikikati)
